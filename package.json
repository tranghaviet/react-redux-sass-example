{
  "name": "react-redux-sass-starterkit",
  "version": "1.1.0",
  "description": "Everything you need to get started with a basic React application",
  "watch": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-datepicker": "^1.5.0",
    "react-device-detect": "^1.4.6",
    "react-dom": "^16.3.2",
    "react-owl-carousel": "^2.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "restrap": "^1.2.2",
    "shx": "^0.3.1",
    "universal-cookie": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "^0.11.2",
    "node-sass": "^4.9.0",
    "pre-commit": "^1.2.2",
    "sass-loader": "^6.0.7",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles --watch --recursive",
    "start": "npm-run-all -p watch-css start:dev",
    "start:dev": "webpack-dev-server",
    "start-dev": "webpack-dev-server --mode development --progress --colors --inline --content-base build",
    "build": "npm run build-css && webpack",
    "watch": "webpack --watch",
    "lint": "./node_modules/.bin/eslint ./src"
  },
  "precommit": "lint",
  "author": "Santhosh Sundar",
  "license": "MIT"
}
